{
  "name": "@project-a/app1-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "app1-e2e",
    "implicitDependencies": [
      "app1"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "packages/app1-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "app1:build",
          "app1:serve"
        ]
      }
    }
  }
}
